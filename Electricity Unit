#include <iostream>
#include<cmath>
using namespace std;
int main()
{
    float unit;
    cin>>unit;
    float res;
    if(unit>=0&&unit<=200)
    {
        res=unit*0.5;
    }
    else if(unit>200&&unit<=400)
    {
        res=(unit*0.65)+100;
    }
    else if(unit>400&&unit<=600)
    {
        res=(unit*0.80)+200;
    }
    else if(unit>600)
    {
        res=(unit*1.25)+425;
    }
    cout<<"Rs."<<ceil(res);
    return 0;
}
